		},
		{
			"type": "shell",
			"label": "Run",
			"command": "./qemu-main",
			"args": [
				
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"detail": "uses: qemu-main",
