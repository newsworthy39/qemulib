			"type": "shell",
			"label": "Build",
			"command": "make",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"args": [
				"all"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "uses compiler: /usr/bin/gcc",
		},
		{
